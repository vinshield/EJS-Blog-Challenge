<%- include('partials/header'); -%>

<h1>Compose</h1>

<form action="/compose" method="post">
  <label for="entryTitle">Title</label>
  <div class="form-group">
    <input type="text" name="postTitle" class="form-control" id="entryTitle" autofocus autocomplete="off">
    <label for="entryText">Post</label>
    <textarea rows="5"  name="postBody" class="form-control" id="entryText"></textarea>
  </div>
  <button type="submit" name="button" class="btn btn-primary">Publish!</button>
</form>

<%- include('partials/footer'); -%>
